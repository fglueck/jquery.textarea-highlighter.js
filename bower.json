{
  "name": "jQuery plugin for highlighting text in textarea",
  "version": "0.3.2",
  "main": "jquery.textarea-highlighter.js",
  "ignore": [
    "**/.*",
    "coffee"
  ],
  "dependencies": {
    "jquery": "~1.8.3",
    "normalize-css": "~2.1.3"
  },
  "exportsOverride": {
    "jquery": {
      "js": "**/*.js"
    },
    "normalize-css": {
      "css": "**/*.css"
    }
  }
}